
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Adicionar empleado</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <script src="https://unpkg.com/gijgo@1.9.13/js/gijgo.min.js" type="text/javascript"></script>
    <link href="https://unpkg.com/gijgo@1.9.13/css/gijgo.min.css" rel="stylesheet" type="text/css" />
    <link href="~/css/styles.css" rel="stylesheet" type="text/css" />
</head>

<body style="background-color: #ffffff">
    <div class="jumbotron" align="center" style="color: #ffffff; background-color: #000000;" contenteditable="false" spellcheckker="false">
        <h1 class="display-3" style="font-family: &quot;Comic Sans MS&quot;, cursive, sans-serif;">MI AUTOMÓVIL</h1>
        <hr class="my-4">
        <p class="lead"></p>
    </div>
    <div class="header clearfix">
        <nav>
            <ul class="nav nav-pills float-right">
                <li class="nav-item">
                    <a class="nav-link active" href="#">Home <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">About</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Contact</a>
                </li>
            </ul>
        </nav>
        <h3 class="text-muted">Registrar empleado</h3>
    </div>
    <br />
    <br />
    <div class="container">
        <div>
            <div class="row">
                <div class="col-sm-12" align="center">
                    <h2>Adicionar empleado</h2>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-3"></div>
                <div class="col-sm-6">
                    <form class="form-horizontal">
                        <fieldset>
                            <div class="form-group">
                                <label class="col-md-4 control-label" for="cedulaEmpleado">Cedula</label>
                                <div class="col-sm-12">
                                    <input id="cedulaEmpleado" name="cedulaEmpleado" type="text" placeholder="" class="form-control input-md">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-4 control-label" for="nombreEmpleado">Nombre</label>
                                <div class="col-sm-12">
                                    <input id="nombreEmpleado" name="nombreEmpleado" type="text" placeholder="" class="form-control input-md">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-4 control-label" for="apellidoEmpleado">Apellido</label>
                                <div class="col-sm-12">
                                    <input id="apellidoEmpleado" name="apellidoEmpleado" type="text" placeholder="" class="form-control input-md">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-4 control-label" for="fechaNacimientoEmpleado">Fecha de nacimiento</label>
                                <div class="col-sm-12">
                                    <input class="col-sm-12" id="datepicker" />
                                    <script>
                                        $('#datepicker').datepicker({
                                            uiLibrary: 'bootstrap4'
                                        });
                                    </script>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-4 control-label" for="cargoEmpleado">Cargo</label>
                                <div class="col-sm-12">
                                    <input id="cargoEmpleado" name="cargoEmpleado" type="text" placeholder="" class="form-control input-md">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-4 control-label" for="concesionarioEmpleado">Concesionario</label>
                                <div class="col-md-12">
                                    <div class="input-group">
                                        <input id="concesionarioEmpleado" name="concesionarioEmpleado" class="form-control" placeholder="" type="text">
                                        <div class="input-group-btn">
                                            <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                                                Seleccionar
                                                <span class="caret"></span>
                                            </button>
                                            <ul class="dropdown-menu pull-right">
                                                <li><a href="#">Option one</a></li>
                                                <li><a href="#">Option two</a></li>
                                                <li><a href="#">Option three</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-4 control-label" for="contraseñaEmpleado">Contraseña</label>
                                <div class="col-sm-12">
                                    <input id="contraseñaEmpleado" name="contraseñaEmpleado" type="password" placeholder="" class="form-control input-md">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-4 control-label" for="repetirContraseñaEmpleado">Repetír contraseña</label>
                                <div class="col-sm-12">
                                    <input id="repetirContraseñaEmpleado" name="repetirContraseñaEmpleado" type="password" placeholder="" class="form-control input-md">
                                </div>
                            </div>
                        </fieldset>
                        <br />
                        <br />
                        <div class="row">
                            <button type="button" class="btn btn-success col-md-4">Guardar</button>
                            <div class="col-md-4"><br /></div>
                            <button type="button" class="btn btn-danger col-md-4">Cancelar</button>
                        </div>
                    </form>
                </div>
                <div class="col-sm-3"></div>
            </div>
        </div>
    </div>
    <br />
    <br />
    <footer class="footer" style="background-color: #000000; color: #fafafa">
        <div>
            <br />
            <br />
            <p>© WYMAN SOLUTION 2019</p>
        </div>
    </footer>
</body>
</html>